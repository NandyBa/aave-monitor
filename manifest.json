{
    "manifest_version": 3,
    "name": "Aave Health Factor",
    "version": "2.5.9",
    "description": "Monitor Aave health factor",
    "action": {
        "default_popup": "js/index.html"
    },
    "permissions": [
        "storage",
        "alarms"
    ],
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "icons": {
        "16": "public/icon16.png",
        "32": "public/icon32.png",
        "128": "public/icon128.png"
    },
    "options_ui": {
        "page": "public/options.html",
        "open_in_tab": true
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "a417bf8d-0c46-4b46-9beb-806cf40e7d09@notrustverify.ch"
        }
    }
}